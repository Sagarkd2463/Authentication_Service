<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login</title>
    <link rel="stylesheet" href="login.css">
</head>

<body>
    <form enctype="application/x-www-form-urlencoded">
        <h2 class="login">Log In</h2>
        <label for="email">Email</label>
        <input type="email" name="email">
        <div class="email error"></div>

        <label for="password">Password</label>
        <input type="password" name="password">
        <div class="password error"></div>

        <button type="submit" class="submit-btn-login">Log In</button>

        <br>
        <hr>
        <header>
            <h3>Log in your account using</h3>
        </header>

        <main>
            <button class="bGoogle">
                <a href="/auth/google" class="google-btn"> 
                    <img src="data:image/png;base64," 
                    alt="Google" />
                    <p class="pGoogle">Sign in with Google</p>
                    </a>
            </button>
            <br>
            <button class="bFacebook">
                <a href="/auth/facebook" class="facebook-btn"> 
                    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQiXN9xSEe8unzPBEQOeAKXd9Q55efGHGB9BA&s" 
                    alt="Facebook" />
                    <p class="pFacebook"> Sign in with Facebook</p>
                   </a>
            </button>
            <br>
            <button class="bGithub">
                <a href="/auth/github" class="github-btn"> 
                    <img src="data:image/png;base64," 
                    alt="Github" />
                    <p class="pGithub">Sign in with Github</p> 
                </a>
            </button>
            <br>
            <button class="bLinkedin">
                <a href="/auth/linkedin" class="linkedin-btn"> 
                    <img src="https://cdn1.iconfinder.com/data/icons/logotypes/32/circle-linkedin-512.png" 
                    alt="LinkedIn" />
                    <p class="pLinkedin">Sign in with LinkedIn</p>
                    </a>
            </button>
        </main>
    </form>
</body>

</html>


<script>
    const form = document.querySelector('form');
    const emailError = document.querySelector('.email.error');
    const passwordError = document.querySelector('.password.error');

    form.addEventListener('submit', async (e) => {
        e.preventDefault();

        emailError.textContent = '';
        passwordError.textContent = '';

        const email = form.email.value;
        const password = form.password.value;

        try {
            const res = await fetch('/login', {
                method: 'POST',
                mode:"cors",
                cache:"no-cache",
                credentials:"same-origin",
                body: JSON.stringify({ email, password }),
                headers: {
                    'Content-type': 'application/json',
                },
            });

            const data = await res.json();
            if (data.error) {
                emailError.textContent = data.error.email;
                passwordError.textContent = data.error.password;
            }

            if (data.user) {
                location.assign('/');
            }

        } catch (err) {
            console.log(err);
        }
    });
</script>