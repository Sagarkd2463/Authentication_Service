<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Login</title>
    <link href="https://cdn.jsdelivr.net/npm/remixicon@4.5.0/fonts/remixicon.css" rel="stylesheet" />
</head>

<style>
    @import url('https://fonts.googleapis.com/css2?family=Quicksand:wght@300;400;500;600;700&display=swap');

    * {
        margin: 0;
        padding: 0;
        box-sizing: border-box;
        font-family: 'Quicksand', Georgia, 'Times New Roman', Times, serif;
    }

    form {
        width: 550px;
        margin: 0 auto;
        margin-top: 15px;
        margin-bottom: 15px;
        padding: 30px;
        box-shadow: 2px 2px 3px rgba(0, 0, 0, 0.5);
        border-radius: 10px;
        background: white;
    }

    .login {
        text-align: center;
    }

    input {
        padding: 10px 12px;
        border-radius: 4px;
        border: 1px solid #ddd;
        font-size: 1em;
        width: 100%;
    }

    label {
        display: block;
        font-weight: bold;
        margin: 20px 0 10px;
    }

    .submit-btn-login {
        margin-top: 30px;
        border-radius: 36px;
        background: black;
        color: white;
        border: 0;
        text-transform: uppercase;
        font-weight: 700;
        font-size: 0.8em;
        display: block;
        padding: 13px 16px;
        letter-spacing: 2px;
        text-align: center;
    }

    .submit-btn-login:hover {
        transition: 0.4ms ease-in;
        transform: scale(1.02);
    }

    #message {
        margin: 10px 0;
        display: inline-block;
        color: white;
        padding: 15px 20px;
        border-radius: 8px;
        font-size: 17px;
        font-weight: 600;
    }

    #eyeicon1 {
        position: absolute;
        font-size: 18px;
        margin-left: -25px;
        margin-top: 12px;
    }

    #eyeicon2 {
        position: absolute;
        font-size: 18px;
        margin-left: -25px;
        margin-top: 12px;
    }

    .account {
        margin-top: 25px;
        font-size: 18px;
        font-weight: 600;
    }

    .btn-signup {
        margin-top: 20px;
        border-radius: 30px;
        background-color: limegreen;
        color: white;
        border: 0;
        text-transform: uppercase;
        text-decoration: none;
        font-weight: 700;
        font-size: 0.9em;
        display: block;
        padding: 15px 16px;
        letter-spacing: 2px;
        text-align: center;
        cursor: pointer;
    }

    .btn-signup:hover {
        transition: 0.5ms ease-in-out;
        transform: scale(1.01);
    }

    header h3 {
        margin-top: 24px;
        text-align: center;
        font-weight: 800;
    }

    main {
        display: flex;
        flex-direction: column;
        align-items: center;
        justify-content: center;
        margin-top: 25px;
        margin-bottom: 25px;
        margin-left: 680px;
        margin-right: 700px;
    }

    .bGoogle {
        margin-top: 10px;
        margin-left: 5px;
        margin-bottom: 20px;
        border-radius: 50px;
        background: #f8f8f8;
        border: none;
        text-transform: uppercase;
        display: flex;
        align-items: center;
        justify-content: center;
        padding: 10px 16px;
        letter-spacing: 2px;
        text-align: center;
    }

    .bGoogle a {
        text-decoration: none;
    }

    .bFacebook {
        margin-top: -15px;
        margin-bottom: -10px;
        margin-left: 30px;
        border-radius: 50px;
        background: #f8f8f8;
        border: none;
        text-transform: uppercase;
        display: flex;
        align-items: center;
        justify-content: center;
        padding: 10px 16px;
        letter-spacing: 2px;
        text-align: center;
    }

    .bFacebook a {
        text-decoration: none;
    }

    .bGithub {
        border-radius: 50px;
        margin-top: 15px;
        margin-bottom: 10px;
        background: #f8f8f8;
        border: none;
        text-transform: uppercase;
        display: flex;
        align-items: center;
        justify-content: center;
        padding: 10px 16px;
        letter-spacing: 2px;
        text-align: center;
    }

    .bGithub a {
        text-decoration: none;
    }

    .bLinkedin {
        margin-top: -10px;
        margin-left: 15px;
        border-radius: 50px;
        background: #f8f8f8;
        border: none;
        text-transform: uppercase;
        display: flex;
        align-items: center;
        justify-content: center;
        padding: 10px 16px;
        letter-spacing: 2px;
        text-align: center;
    }

    .bLinkedin a {
        text-decoration: none;
    }

    .google-btn {
        padding-left: 5px;
        padding-top: 5px;
        padding-right: 5px;
        padding-bottom: 5px;
        color: limegreen;
    }

    .google-btn img {
        width: 25px;
        height: 25px;
        margin-left: -260px;
        margin-right: 60px;
        margin-bottom: -16px;
    }

    .google-btn .pGoogle {
        margin-left: 5px;
        margin-top: -10px;
        font-size: 18px;
        font-weight: 500;
    }

    .facebook-btn {
        padding-left: 5px;
        padding-top: 5px;
        padding-right: 5px;
        padding-bottom: 5px;
        color: blue;
    }

    .facebook-btn img {
        width: 25px;
        height: 25px;
        margin-left: -290px;
        margin-right: 60px;
        margin-bottom: -16px;
    }

    .facebook-btn .pFacebook {
        margin-left: 5px;
        margin-top: -10px;
        font-size: 18px;
        font-weight: 500;
    }

    .github-btn {
        padding-left: 5px;
        padding-top: 5px;
        padding-right: 5px;
        padding-bottom: 5px;
        color: black;
    }

    .github-btn img {
        width: 25px;
        height: 25px;
        margin-left: -260px;
        margin-right: 60px;
        margin-bottom: -16px;
    }

    .github-btn .pGithub {
        margin-left: 5px;
        margin-top: -10px;
        font-size: 18px;
        font-weight: 500;
    }

    .linkedin-btn {
        padding-left: 5px;
        padding-top: 5px;
        padding-right: 5px;
        padding-bottom: 5px;
        color: lightseagreen;
    }

    .linkedin-btn img {
        width: 25px;
        height: 25px;
        margin-left: -280px;
        margin-right: 60px;
        margin-bottom: -16px;
    }

    .linkedin-btn .pLinkedin {
        margin-left: 5px;
        margin-top: -10px;
        font-size: 18px;
        font-weight: 500;
    }
</style>

<body>
    <form action="/login" method="post">
        <h2 class="login">Log In</h2>
        <label for="email">Email</label>
        <input type="email" name="email" placeholder="Email" required>

        <div>
            <label for="password">Password</label>
            <input type="password" name="password" placeholder="Password" id="password" required>
            <i class="ri-eye-off-fill" id="eyeicon1"></i>
        </div>

        <div>
            <label for="confirm_password">Confirm Password</label>
            <input type="password" name="password" placeholder="Password" id="confirmPassword" required>
            <i class="ri-eye-off-fill" id="eyeicon2"></i>
        </div>

        <p id="message"></p>

        <button type="submit" class="submit-btn-login" onclick="checkPassword()">Log In</button>

        <p class="account">Don't have an account?</p>
        <a href="/register" class="btn-signup">Sign Up</a>
    </form>

    <header>
        <h3>Log in your account using</h3>
    </header>

    <main>
        <button class="bGoogle">
            <a href="http://localhost:5000/auth/google" class="google-btn">
                <img src="data:image/png;base64,"
                    alt="Google" />
                <p class="pGoogle">Log in with Google</p>
            </a>
        </button>
        <br>
        <button class="bFacebook">
            <a href="http://localhost:5000/auth/facebook" class="facebook-btn">
                <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQiXN9xSEe8unzPBEQOeAKXd9Q55efGHGB9BA&s"
                    alt="Facebook" />
                <p class="pFacebook">Log in with Facebook</p>
            </a>
        </button>
        <br>
        <button class="bGithub">
            <a href="http://localhost:5000/auth/github" class="github-btn">
                <img src="data:image/png;base64,"
                    alt="Github" />
                <p class="pGithub">Log in with Github</p>
            </a>
        </button>
        <br>
        <button class="bLinkedin">
            <a href="http://localhost:5000/auth/linkedin" class="linkedin-btn">
                <img src="https://cdn1.iconfinder.com/data/icons/logotypes/32/circle-linkedin-512.png" alt="LinkedIn" />
                <p class="pLinkedin">Log in with LinkedIn</p>
            </a>
        </button>
    </main>
</body>

<script>

    const checkPassword = () => {

        let password = document.getElementById("password").value;
        let confirmPassword = document.getElementById("confirmPassword").value;

        let message = document.getElementById("message");

        if (password.length !== 0) {
            if (password === confirmPassword) {
                message.textContent = "Passwords are matching";
                message.style.backgroundColor = "#008000";
            } else {
                message.textContent = "Passwords don't match";
                message.style.backgroundColor = "#ff0000";
            }
        } else {
            message.textContent = "";
        }
    };

    let eyeicon1 = document.getElementById("eyeicon1");
    let eyeicon2 = document.getElementById("eyeicon2");
    let password = document.getElementById("password");
    let confirmPassword = document.getElementById("confirmPassword");

    eyeicon1.onclick = () => {

        if (password.type === "password") {
            password.type = "text";
            eyeicon1.className = "ri-eye-fill";
        } else {
            password.type = "password";
            eyeicon1.className = "ri-eye-off-fill";
        }
    };

    eyeicon2.onclick = () => {

        if (confirmPassword.type === "password") {
            confirmPassword.type = "text";
            eyeicon2.className = "ri-eye-fill";
        } else {
            confirmPassword.type = "password";
            eyeicon2.className = "ri-eye-off-fill";
        }
    };
</script>

</html>